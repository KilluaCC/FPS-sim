{
  "name": "fps-estimator",
  "version": "1.0.0",
  "description": "GPU/CPU FPS Estimator - A modern web app for estimating game performance",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "cd database && npm run db:seed",
    "db:studio": "prisma studio",
    "db:setup": "cd database && ./setup.sh"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "@prisma/client": "^5.7.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "prisma": "^5.7.1"
  },
  "keywords": ["fps", "gpu", "cpu", "gaming", "performance"],
  "author": "Your Name",
  "license": "MIT"
}
