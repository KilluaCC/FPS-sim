{
  "name": "fps-estimator",
  "version": "1.0.0",
  "description": "GPU/CPU FPS Estimator - A modern web app for estimating game performance",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["fps", "gpu", "cpu", "gaming", "performance"],
  "author": "Your Name",
  "license": "MIT"
}
